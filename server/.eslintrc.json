{
  "env":{
    "browser":true,
    "es2021":true,
    "node":true
  },
  "extends":[
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser":"@typescript-eslint/parser",
  "parserOptions":{
    "ecmaVersion":12,
    "sourceType":"module"
  },
  "plugins":[
    "@typescript-eslint"
  ],
  "rules":{
    "@typescript-eslint/no-unused-vars":"error",
    "@typescript-eslint/no-explicit-any":"error",
    "prettier/prettier":0,
    "no-console":"error"
  }
}